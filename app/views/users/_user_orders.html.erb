<p id="notice"><%= notice %></p>

<h1>Listing Orders</h1>

<table class="table">
  <thead>
    <tr>
      <th>Number</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @o.each do |order| %>
      <tr>
        <td><%= order.number %></td>
        <td><%= order.user.try(:name) %></td>
       </tr>
    <% end %>
  </tbody>
</table>

<br>

